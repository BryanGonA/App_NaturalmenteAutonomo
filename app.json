{
  "expo": {
    "name": "App_Bienestar",
    "slug": "App_Bienestar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundImage": "./src/assets/backg_1.jpg"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicaci√≥n accede a tus fotos para que puedas compartir con tus amigos."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "backgroundImage": "./src/assets/backg_1.jpg"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF",
        "backgroundImage": "./src/assets/backg_1.jpg"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
